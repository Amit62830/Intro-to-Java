import java.util.*;
public class Solution {

	public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int i = 1;
        // int a = n;
        while( i <= n ){
            int j = 1;
            while(j <= n - i + 1){
                System.out.print(n - i + 1);
                j++;
            }
            System.out.println();
            i++;
            // a--;
        }

	}

}
